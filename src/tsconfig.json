{
   "compilerOptions": {
     "target": "ES6",
     "module": "ESNext",
     "lib": ["DOM", "ES6", "ES2017"],
     "jsx": "react",
     "moduleResolution": "node",
     "outDir": "../dist",
     "rootDir": ".",
     "strict": true,
     "esModuleInterop": true,
     "skipLibCheck": true,
     "forceConsistentCasingInFileNames": true
   },
   "include": ["**/*"],
   "exclude": ["../dist"]
 }
 